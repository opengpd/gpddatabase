{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Schema used by gpddatabase: data_types.yaml",
    "type": "object",
    "properties": {



        "data": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {

                    "name": {
                        "type": "string"
                    },

                    "description": {
                        "type": "string"
                    }

                },
                "required": ["name", "description"],
                "additionalProperties": false
            }
        }



    },
    "required": ["data"],
    "additionalProperties": false
}
